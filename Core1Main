package core.lesson2;

public class Main {

	public static void main(String[] args) {
		Person p = new Person();
		p.age = 20;
		p.name = "Ivan";
		System.out.println(p.age);
		System.out.println(p.name);
		Person p1 = new Person();
		p1.age = 18;
		p1.name = "Petro";
		System.out.println(p1.age);
		System.out.println(p1.name);

	}

}
